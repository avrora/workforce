{
  "variables" : [
    {
      "name" : "modelName",
      "description" : "What's your model called (Mongo.Collection)?"
    }
  ],
  "files" : [
    {
      "template" : "model.coffee",
      "path" : "models/__modelName__.coffee"
    },
    {
      "template" : "publication.coffee",
      "path" : "server/publications/__modelName__Pub.coffee"
    }
  ],
  "notice" : "Dont forget to add Meteor.subscribe('__modelName__'); to your route!"
}