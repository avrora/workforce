{
  "variables" : [
    {
      "name" : "modelName",
      "description" : "What's your model called (Mongo.Collection)?"
    }
  ],
  "files" : [
    {
      "template" : "model.next.js",
      "path" : "models/__modelName__.next.js"
    },
    {
      "template" : "publication.next.js",
      "path" : "server/publications/__modelName__Pub.next.js"
    }
  ],
  "notice" : "Dont forget to add Meteor.subscribe('__modelName__'); to your route!"
}
